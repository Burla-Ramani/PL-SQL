SELECT * FROM EMP WHERE HIREDATE BETWEEN '1-DEC-1980' AND '1-DEC-1981' AND COMM IS NOT NULL ORDER BY 1;
SELECT EMPNO,ENAME,SAL,HIREDATE,ADD_MONTHS(HIREDATE,10)AS EXTENDING_DATE FROM EMP;
SELECT EMPNO,ENAME,SAL,HIREDATE,ADD_MONTHS(HIREDATE,-10)AS EXTENDING_DATE FROM EMP;
SELECT EMPNO,ENAME,SAL,HIREDATE,NEXT_DAY(HIREDATE,'MONDAY')AS EXTENDING_DATES FROM EMP;
SELECT EMPNO,ENAME,SAL,HIREDATE,NEXT_DAY(HIREDATE,1)AS EXTENDING_DATES FROM EMP;
SELECT EMPNO,ENAME,SAL,HIREDATE,LAST_DAY(HIREDATE)AS EXTENDING_DATES FROM EMP;
SELECT ENAME,SAL,TO_CHAR(HIREDATE,'DD-MM-YYYY')FROM EMP;
SELECT ENAME,SAL,HIREDATE ,TO_CHAR(HIREDATE,'DD-MM-YYYY')AS TOCHAR FROM EMP;
SELECT ENAME,SAL,TO_DATE(SAL,'DD-MM-YYYY')FROM EMP;
